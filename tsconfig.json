{
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "strict": true,
        "jsx": "preserve",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "lib": [
            "ESNext",
            "DOM"
        ],
        "skipLibCheck": true,
        "noEmit": true,
        "baseUrl": ".",
        "paths": {
            "@": [
                "src"
            ],
            "@/*": [
                "src/*"
            ],
        },
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.d.ts",
        "src/**/*.tsx",
        "src/**/*.vue"
, "src/utils/lang/zh.ts", "src/utils/lang/zh.ts", "src/utils/lang/index.ts", "src/utils/lang/en.ts"    ],
    "references": [
        {
            "path": "./tsconfig.node.json"
        }
    ]
}